{
	"compilerOptions": {
		"module": "ESNext",
		"target": "ESNext",
		"outDir": "bin/",
		"moduleResolution": "Node16",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"strict": false,
		"skipLibCheck": true,
		"sourceMap": true,
	},
	"include": ["src/**/*"]
}
